{
  "name": "x-package-version-strict-check",
  "version": "1.7.0",
  "description": "A tool to check package's version strictly in project.",
  "main": "./dist/index.js",
  "scripts": {
    "clear": "del-cli ./dist/**",
    "start": "ts-node index.ts",
    "test": "ts-node index.ts --path ./",
    "dist": "npm run clear && tsc",
    "release": "standard-version && git push && git push --tags",
    "ct": "git add -A && git-cz -a"
  },
  "bin": {
    "x-package-version-strict-check": "./dist/cmd.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xucongli1989/x-package-version-strict-check.git"
  },
  "keywords": [
    "node",
    "package",
    "version",
    "check",
    "compare",
    "version strict compare"
  ],
  "files": [
    "dist"
  ],
  "author": "XCL",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/xucongli1989/x-package-version-strict-check/issues"
  },
  "homepage": "https://github.com/xucongli1989/x-package-version-strict-check#readme",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=6.0.0"
  },
  "devDependencies": {
    "@types/commander": "2.12.2",
    "@types/node": "10.12.18",
    "@types/semver": "6.0.0",
    "del-cli": "1.1.0",
    "ts-node": "8.0.3",
    "typescript": "3.4.3",
    "commitizen": "4.2.2",
    "standard-version": "9.0.0"
  },
  "dependencies": {
    "commander": "2.20.0",
    "semver": "6.0.0"
  }
}
